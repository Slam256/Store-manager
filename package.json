    "url": "https://github.com/Slam256/Store-manager/issues"
  },
  "homepage": "https://github.com/Slam256/Store-manager#readme",
  "dependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "nodemon": "^2.0.19"
  },
  "devDependencies": {
    "@babel/register": "^7.18.9",
    "husky": "^8.0.1",
